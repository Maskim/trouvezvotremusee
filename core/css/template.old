@import url("fonts.css");
@import url("http://fonts.googleapis.com/css?family=Lobster");

body { margin:0;padding:0;background:#f5f2ed url("../img/bg.jpg");min-width:900px;max-height:800px; }
 
/* CONNEXION */
#connexion { float: right; background: #212121;  padding: 6px 10px; margin-right: 20px; height: 40px;
	-webkit-border-bottom-left-radius: 10px; z-index: 1; position: relative;
	-moz-border-radius-bottomleft: 10px;
	border-bottom-left-radius: 10px; margin-top: -52px;}
#connexion p { display: inline; color: white; }
#connexion a { font-family:Arial,Helvetica,sans-serif;font-size:12px;color:#4fb7d4;text-decoration:none;letter-spacing:0px;text-shadow: 0 1px 0 #000000; }
#connexion a:hover, a:active, a:visited { color:white;outline-style:none; }
#connexion .submit{ text-align: right; }
#connexion .input{ text-align: right; padding-bottom: 5px;}
#connexion .zoneConnexion{ float: right;margin-top: 10px; background: #212121; padding: 15px 10px 3px 10px;
	-webkit-border-bottom-right-radius: 10px; -webkit-border-bottom-left-radius: 10px;
	-moz-border-radius-bottomright: 10px; -moz-border-radius-bottomleft: 10px;
	border-bottom-right-radius: 10px; border-bottom-left-radius: 10px; position: absolute; right: -3px;}
#connexion ul{ margin-top: 0px; margin-bottom: 6px;}
#connexion li{ display: inline; color: white; }

/* EN TÊTE */	
#head { margin:0;padding:0;height:125px;background:#212121; border-bottom:6px solid #e0e0df; position: relative; z-index: 2;}
#head #en-tete { width:900px;margin:0 auto;  }
#head #en-tete #logo { background:url("../img/logo.png") no-repeat;background-position:left top;width:573px;height:106px;float:left;margin:7px 80px 0 40px; }
