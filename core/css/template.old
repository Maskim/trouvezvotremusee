#head #en-tete #logo a { width:561px;height:95px;display:block; }
#head #en-tete #logo a:active, a:visited { outline-style:none; }
#head #en-tete #navigation ul { height:105px;list-style-type: none;margin:0;padding:22px 0 0 0; }
#head #en-tete #navigation a { font-family:Arial,Helvetica,sans-serif;font-size:12px;color:#4fb7d4;text-decoration:none;letter-spacing:0px;text-shadow: 0 1px 0 #000000; }
